{
  "name": "Apple Filing Protocol (netatalk)",
  "version": "0.0.17",
  "description": "Serve the supervisor's shares as AFP.",
  "url": "https://github.com/marcelveldt/hassio-addons-repo/netatalk",
  "slug": "netatalk",
  "stage": "experimental",
  "startup": "application",
  "boot": "auto",
  "init": "false",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "environment": {},
  "ports": {
    "548/tcp": 548
  },
  "map": ["config:rw", "ssl:rw", "addons:rw", "share:rw", "backup:rw", "media:rw"],
  "options": {
    "username": "afp",
    "password": "afp"
  },
  "schema": {
    "username": "str",
    "password": "str"
  }
}
