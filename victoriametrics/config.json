{
  "name": "Victoria Metrics",
  "version": "1.84.0",
  "slug": "victoria_metrics",
  "stage": "experimental",
  "description": "Local Victoria Metrics TSDB",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "map": ["config:rw"],
  "init": "false",
  "ingress": true,
  "ingress_port": 8428,
  "options": {
    "retention": "6"
  },
  "schema": {
    "retention": "str"
  },
  "webui": "http://[HOST]:[PORT:8428]/",
  "ports": {
    "8428/tcp": 8428
  }
}
